{
  "name": "@mgcrea/react-native-select",
  "version": "1.0.2",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn build:source && yarn build:typings",
    "build:source": "NODE_ENV=production babel --verbose --source-maps --extensions .js,.ts,.tsx --out-dir lib/ --delete-dir-on-start --ignore **/__tests__ src/",
    "build:typings": "NODE_ENV=production tsc --declaration",
    "typecheck": "tsc --pretty --noEmit",
    "test": "CI=true jest --runInBand --colors"
  },
  "dependencies": {
    "@mgcrea/react-native-button": "1.0.2",
    "@mgcrea/react-native-modal-dialog": "1.0.2"
  },
  "peerDependencies": {
    "@babel/runtime": ">=7",
    "@react-native-picker/picker": ">=2",
    "react": ">=16.8",
    "react-native": ">=0.59"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/runtime": "^7.16.7",
    "@react-native-picker/picker": "^2.2.1",
    "@storybook/react-native": "^6.0.1-alpha.6",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.12",
    "@types/react": "^17.0.38",
    "@types/react-native": "^0.66.15",
    "@types/react-test-renderer": "^17.0.1",
    "react": "^17.0.2",
    "react-native": "^0.66.4",
    "react-native-testing-library": "^2.2.0",
    "react-test-renderer": "^17.0.2",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.5"
  }
}
